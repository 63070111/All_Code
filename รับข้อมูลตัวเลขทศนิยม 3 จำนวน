#include<stdio.h>
double swap(double *a, double *b, double *c){
	double full,save;
	full = *a;
	save = *b;
	*a = *c;
	*b = full;
	*c = save;
}


int main() {
  double a,b,c;
  scanf("%lf %lf %lf",&a,&b,&c);
  swap(&a,&b,&c);
  printf("%.2lf %.2lf %.2lf",a,b,c);
  return 0;
}
/* เขียนโปรแกรม เพื่อรับข้อมูลตัวเลขทศนิยม 3 จำนวน ใน 1 บรรทัด จากนั้นให้ส่งเลขทศนิยมทั้ง 3 ค่า ไปยังฟังก์ชันเพื่อทำการสลับตำแหน่งของตัวเลขทั้งสาม โดยมีเงื่อนไขดังนี้
ให้ตัวแรกไปอยู่ตำแหน่งที่สอง
ให้ตัวที่สองไปอยู่ตำแหน่งที่สาม
ให้ตัวที่สามไปอยู่ตำแหน่งแรก
ให้ใช้ pointer ในการสลับตำแหน่ง โดยให้สร้างฟังก์ชันเพื่อทำการสลับ
หมายเหตุ:
เลขทศนิยมให้ใช้ประเภท double
หากไม่ใช้ pointer จะไม่นำมาคิดคะแนน
